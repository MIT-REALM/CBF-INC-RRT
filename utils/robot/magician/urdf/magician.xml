<robot name="magician">

    <!-- Defining the colors used in this robot -->
    <material name="Black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="White">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="Grey">
        <color rgba="0.9 0.9 0.9 1"/>
    </material>

    <!--  -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://magician/meshes/base_link.STL"/>
            </geometry>
            <material name="White"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://magician/meshes/base_link.STL"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint_1" type ="revolute">
        <origin xyz="0 0 0.024" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="link_1"/>
        <axis xyz="0 0 1"/>
        <limit effort="150.0" lower="-3.14159265" upper="3.14159265" velocity="3.15"/>
    </joint>

    <link name="link_1">
